{
  "parser": "babel-eslint",
  "extends": ["standard", "standard-react"],
  "plugins": ["flowtype"],
  "rules": {
    "space-before-function-paren": "off",
    "jsx-quotes": ["error", "prefer-double"],
    "comma-dangle": ["error", "always-multiline"],
    "generator-star-spacing": "off"
  },
  "globals": {
    "localStorage": true,
    "fetch": true,
    "location": true
  }
}